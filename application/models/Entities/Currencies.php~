<?php



/**
 * Currencies
 *
 * @Table(name="currencies")
 * @Entity
 */
class Currencies
{
    /**
     * @var integer $idCurrency
     *
     * @Column(name="id_currency", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCurrency;

    /**
     * @var string $currencyName
     *
     * @Column(name="currency_name", type="string", length=32, nullable=false)
     */
    private $currencyName;

    /**
     * @var string $currencyDetail
     *
     * @Column(name="currency_detail", type="string", length=32, nullable=true)
     */
    private $currencyDetail;


}