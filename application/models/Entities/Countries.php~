<?php



/**
 * Countries
 *
 * @Table(name="countries")
 * @Entity
 */
class Countries
{
    /**
     * @var integer $idCountry
     *
     * @Column(name="id_country", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCountry;

    /**
     * @var string $countryName
     *
     * @Column(name="country_name", type="string", length=32, nullable=false)
     */
    private $countryName;

    /**
     * @var string $zipCode
     *
     * @Column(name="zip_code", type="string", length=32, nullable=true)
     */
    private $zipCode;


}