<?php



/**
 * ClassifiedItems
 *
 * @Table(name="classified_items")
 * @Entity
 */
class ClassifiedItems
{
    /**
     * @var integer $idClassifiedItem
     *
     * @Column(name="id_classified_item", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idClassifiedItem;

    /**
     * @var Products
     *
     * @ManyToOne(targetEntity="Products")
     * @JoinColumns({
     *   @JoinColumn(name="fid_product", referencedColumnName="id_product")
     * })
     */
    private $fidProduct;

    /**
     * @var Sellers
     *
     * @ManyToOne(targetEntity="Sellers")
     * @JoinColumns({
     *   @JoinColumn(name="fid_seller", referencedColumnName="id_seller")
     * })
     */
    private $fidSeller;


}