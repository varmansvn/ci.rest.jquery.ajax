<?php



/**
 * CompanyProfiles
 *
 * @Table(name="company_profiles")
 * @Entity
 */
class CompanyProfiles
{
    /**
     * @var integer $idCompanyProfile
     *
     * @Column(name="id_company_profile", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCompanyProfile;

    /**
     * @var string $logo
     *
     * @Column(name="logo", type="string", length=64, nullable=false)
     */
    private $logo;

    /**
     * @var string $background
     *
     * @Column(name="background", type="string", length=128, nullable=true)
     */
    private $background;

    /**
     * @var string $branches
     *
     * @Column(name="branches", type="string", length=64, nullable=true)
     */
    private $branches;

    /**
     * @var GenericProfiles
     *
     * @ManyToOne(targetEntity="GenericProfiles")
     * @JoinColumns({
     *   @JoinColumn(name="fid_generic_profile", referencedColumnName="id_generic_profile")
     * })
     */
    private $fidGenericProfile;

    /**
     * @var Users
     *
     * @ManyToOne(targetEntity="Users")
     * @JoinColumns({
     *   @JoinColumn(name="fid_user", referencedColumnName="id_user")
     * })
     */
    private $fidUser;


}