<?php



/**
 * Products
 *
 * @Table(name="products")
 * @Entity
 */
class Products
{
    /**
     * @var integer $idProduct
     *
     * @Column(name="id_product", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idProduct;

    /**
     * @var datetime $createdAt
     *
     * @Column(name="created_at", type="datetime", nullable=true)
     */
    private $createdAt;

    /**
     * @var datetime $updatedAt
     *
     * @Column(name="updated_at", type="datetime", nullable=true)
     */
    private $updatedAt;

    /**
     * @var ProductClasses
     *
     * @ManyToOne(targetEntity="ProductClasses")
     * @JoinColumns({
     *   @JoinColumn(name="fid_product_class", referencedColumnName="id_product_class")
     * })
     */
    private $fidProductClass;

    /**
     * @var ProductStatusCodes
     *
     * @ManyToOne(targetEntity="ProductStatusCodes")
     * @JoinColumns({
     *   @JoinColumn(name="fid_product_status_code", referencedColumnName="id_product_status_code")
     * })
     */
    private $fidProductStatusCode;


}