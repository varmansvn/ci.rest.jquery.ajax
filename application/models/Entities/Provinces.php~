<?php



/**
 * Provinces
 *
 * @Table(name="provinces")
 * @Entity
 */
class Provinces
{
    /**
     * @var integer $idProvince
     *
     * @Column(name="id_province", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idProvince;

    /**
     * @var string $provinceName
     *
     * @Column(name="province_name", type="string", length=32, nullable=false)
     */
    private $provinceName;

    /**
     * @var string $zipCode
     *
     * @Column(name="zip_code", type="string", length=32, nullable=true)
     */
    private $zipCode;

    /**
     * @var States
     *
     * @ManyToOne(targetEntity="States")
     * @JoinColumns({
     *   @JoinColumn(name="fid_state", referencedColumnName="id_state")
     * })
     */
    private $fidState;


}