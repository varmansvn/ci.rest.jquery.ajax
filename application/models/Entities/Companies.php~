<?php



/**
 * Companies
 *
 * @Table(name="companies")
 * @Entity
 */
class Companies
{
    /**
     * @var integer $idCompany
     *
     * @Column(name="id_company", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCompany;

    /**
     * @var CompanyProfiles
     *
     * @ManyToOne(targetEntity="CompanyProfiles")
     * @JoinColumns({
     *   @JoinColumn(name="fid_company_profile", referencedColumnName="id_company_profile")
     * })
     */
    private $fidCompanyProfile;

    /**
     * @var CompanyStatusCodes
     *
     * @ManyToOne(targetEntity="CompanyStatusCodes")
     * @JoinColumns({
     *   @JoinColumn(name="fid_company_status_code", referencedColumnName="id_company_status_code")
     * })
     */
    private $fidCompanyStatusCode;


}