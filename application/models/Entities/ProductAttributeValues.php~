<?php



/**
 * ProductAttributeValues
 *
 * @Table(name="product_attribute_values")
 * @Entity
 */
class ProductAttributeValues
{
    /**
     * @var integer $idProductAttributeValues
     *
     * @Column(name="id_product_attribute_values", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idProductAttributeValues;

    /**
     * @var string $attributeValue
     *
     * @Column(name="attribute_value", type="string", length=32, nullable=false)
     */
    private $attributeValue;

    /**
     * @var ProductAttributes
     *
     * @ManyToOne(targetEntity="ProductAttributes")
     * @JoinColumns({
     *   @JoinColumn(name="fid_product_attribute", referencedColumnName="id_product_attribute")
     * })
     */
    private $fidProductAttribute;


}