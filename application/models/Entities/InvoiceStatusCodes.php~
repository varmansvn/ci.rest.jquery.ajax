<?php



/**
 * InvoiceStatusCodes
 *
 * @Table(name="invoice_status_codes")
 * @Entity
 */
class InvoiceStatusCodes
{
    /**
     * @var integer $idInvoiceStatusCode
     *
     * @Column(name="id_invoice_status_code", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idInvoiceStatusCode;

    /**
     * @var string $invoiceStatusCodeDescription
     *
     * @Column(name="invoice_status_code_description", type="string", length=32, nullable=false)
     */
    private $invoiceStatusCodeDescription;


}