<?php



/**
 * Districts
 *
 * @Table(name="districts")
 * @Entity
 */
class Districts
{
    /**
     * @var integer $idDistrict
     *
     * @Column(name="id_district", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idDistrict;

    /**
     * @var string $districtName
     *
     * @Column(name="district_name", type="string", length=32, nullable=false)
     */
    private $districtName;

    /**
     * @var string $zipCode
     *
     * @Column(name="zip_code", type="string", length=32, nullable=true)
     */
    private $zipCode;

    /**
     * @var Provinces
     *
     * @ManyToOne(targetEntity="Provinces")
     * @JoinColumns({
     *   @JoinColumn(name="fid_province", referencedColumnName="id_province")
     * })
     */
    private $fidProvince;


}