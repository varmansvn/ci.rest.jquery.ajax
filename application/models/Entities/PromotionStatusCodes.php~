<?php



/**
 * PromotionStatusCodes
 *
 * @Table(name="promotion_status_codes")
 * @Entity
 */
class PromotionStatusCodes
{
    /**
     * @var integer $idPromotionStatusCode
     *
     * @Column(name="id_promotion_status_code", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idPromotionStatusCode;

    /**
     * @var string $promotionStatusValue
     *
     * @Column(name="promotion_status_value", type="string", length=32, nullable=true)
     */
    private $promotionStatusValue;


}