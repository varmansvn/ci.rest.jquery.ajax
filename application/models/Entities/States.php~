<?php



/**
 * States
 *
 * @Table(name="states")
 * @Entity
 */
class States
{
    /**
     * @var integer $idState
     *
     * @Column(name="id_state", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idState;

    /**
     * @var string $stateName
     *
     * @Column(name="state_name", type="string", length=32, nullable=false)
     */
    private $stateName;

    /**
     * @var string $zipCode
     *
     * @Column(name="zip_code", type="string", length=32, nullable=true)
     */
    private $zipCode;

    /**
     * @var Countries
     *
     * @ManyToOne(targetEntity="Countries")
     * @JoinColumns({
     *   @JoinColumn(name="fid_country", referencedColumnName="id_country")
     * })
     */
    private $fidCountry;


}