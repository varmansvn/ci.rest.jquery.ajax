<?php



/**
 * Shipments
 *
 * @Table(name="shipments")
 * @Entity
 */
class Shipments
{
    /**
     * @var integer $idShipment
     *
     * @Column(name="id_shipment", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idShipment;

    /**
     * @var string $shipmentTrackingNumber
     *
     * @Column(name="shipment_tracking_number", type="string", length=128, nullable=false)
     */
    private $shipmentTrackingNumber;

    /**
     * @var datetime $shippedAt
     *
     * @Column(name="shipped_at", type="datetime", nullable=true)
     */
    private $shippedAt;

    /**
     * @var string $shipmentDetail
     *
     * @Column(name="shipment_detail", type="string", length=128, nullable=true)
     */
    private $shipmentDetail;

    /**
     * @var datetime $createdAt
     *
     * @Column(name="created_at", type="datetime", nullable=true)
     */
    private $createdAt;

    /**
     * @var OrderItems
     *
     * @ManyToMany(targetEntity="OrderItems", inversedBy="fidShipment")
     * @JoinTable(name="shipment_items",
     *   joinColumns={
     *     @JoinColumn(name="fid_shipment", referencedColumnName="id_shipment")
     *   },
     *   inverseJoinColumns={
     *     @JoinColumn(name="fid_order_item", referencedColumnName="id_order_items")
     *   }
     * )
     */
    private $fidOrderItem;

    /**
     * @var Orders
     *
     * @ManyToOne(targetEntity="Orders")
     * @JoinColumns({
     *   @JoinColumn(name="fid_order", referencedColumnName="id_order")
     * })
     */
    private $fidOrder;

    /**
     * @var Invoices
     *
     * @ManyToOne(targetEntity="Invoices")
     * @JoinColumns({
     *   @JoinColumn(name="fid_invoice", referencedColumnName="id_invoice")
     * })
     */
    private $fidInvoice;

    public function __construct()
    {
        $this->fidOrderItem = new \Doctrine\Common\Collections\ArrayCollection();
    }
    
}