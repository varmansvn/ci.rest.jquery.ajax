<?php



/**
 * ExchangeRates
 *
 * @Table(name="exchange_rates")
 * @Entity
 */
class ExchangeRates
{
    /**
     * @var integer $id
     *
     * @Column(name="id", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var integer $rate
     *
     * @Column(name="rate", type="integer", nullable=false)
     */
    private $rate;

    /**
     * @var datetime $updatedAt
     *
     * @Column(name="updated_at", type="datetime", nullable=false)
     */
    private $updatedAt;

    /**
     * @var Currencies
     *
     * @ManyToOne(targetEntity="Currencies")
     * @JoinColumns({
     *   @JoinColumn(name="fid_from_currency", referencedColumnName="id_currency")
     * })
     */
    private $fidFromCurrency;

    /**
     * @var Currencies
     *
     * @ManyToOne(targetEntity="Currencies")
     * @JoinColumns({
     *   @JoinColumn(name="fid_to_currency", referencedColumnName="id_currency")
     * })
     */
    private $fidToCurrency;


}