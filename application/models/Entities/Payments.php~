<?php



/**
 * Payments
 *
 * @Table(name="payments")
 * @Entity
 */
class Payments
{
    /**
     * @var integer $idPayment
     *
     * @Column(name="id_payment", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idPayment;

    /**
     * @var datetime $paidAt
     *
     * @Column(name="paid_at", type="datetime", nullable=false)
     */
    private $paidAt;

    /**
     * @var integer $paymentAmount
     *
     * @Column(name="payment_amount", type="integer", nullable=false)
     */
    private $paymentAmount;

    /**
     * @var Currencies
     *
     * @ManyToOne(targetEntity="Currencies")
     * @JoinColumns({
     *   @JoinColumn(name="fid_currency", referencedColumnName="id_currency")
     * })
     */
    private $fidCurrency;

    /**
     * @var Invoices
     *
     * @ManyToOne(targetEntity="Invoices")
     * @JoinColumns({
     *   @JoinColumn(name="fid_invoice", referencedColumnName="id_invoice")
     * })
     */
    private $fidInvoice;


}