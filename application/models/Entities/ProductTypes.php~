<?php



/**
 * ProductTypes
 *
 * @Table(name="product_types")
 * @Entity
 */
class ProductTypes
{
    /**
     * @var integer $idProductType
     *
     * @Column(name="id_product_type", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idProductType;

    /**
     * @var string $productTypeValue
     *
     * @Column(name="product_type_value", type="string", length=32, nullable=true)
     */
    private $productTypeValue;


}