<?php



/**
 * CompanySectors
 *
 * @Table(name="company_sectors")
 * @Entity
 */
class CompanySectors
{
    /**
     * @var integer $idCompanySectors
     *
     * @Column(name="id_company_sectors", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCompanySectors;

    /**
     * @var Sectors
     *
     * @ManyToOne(targetEntity="Sectors")
     * @JoinColumns({
     *   @JoinColumn(name="fid_sector", referencedColumnName="id_sector")
     * })
     */
    private $fidSector;

    /**
     * @var Companies
     *
     * @ManyToOne(targetEntity="Companies")
     * @JoinColumns({
     *   @JoinColumn(name="fid_company", referencedColumnName="id_company")
     * })
     */
    private $fidCompany;


}