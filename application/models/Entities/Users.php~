<?php



/**
 * Users
 *
 * @Table(name="users")
 * @Entity
 */
class Users
{
    /**
     * @var integer $idUser
     *
     * @Column(name="id_user", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idUser;

    /**
     * @var string $username
     *
     * @Column(name="username", type="string", length=32, nullable=false)
     */
    private $username;

    /**
     * @var string $password
     *
     * @Column(name="password", type="string", length=32, nullable=false)
     */
    private $password;

    /**
     * @var datetime $lastLogin
     *
     * @Column(name="last_login", type="datetime", nullable=true)
     */
    private $lastLogin;

    /**
     * @var UserStatusCodes
     *
     * @ManyToOne(targetEntity="UserStatusCodes")
     * @JoinColumns({
     *   @JoinColumn(name="fid_user_status_code", referencedColumnName="id_user_status_code")
     * })
     */
    private $fidUserStatusCode;


}