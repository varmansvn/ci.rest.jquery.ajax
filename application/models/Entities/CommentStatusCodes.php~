<?php



/**
 * CommentStatusCodes
 *
 * @Table(name="comment_status_codes")
 * @Entity
 */
class CommentStatusCodes
{
    /**
     * @var integer $idCommentStatusCode
     *
     * @Column(name="id_comment_status_code", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCommentStatusCode;

    /**
     * @var string $commentStatusValue
     *
     * @Column(name="comment_status_value", type="string", length=32, nullable=true)
     */
    private $commentStatusValue;


}