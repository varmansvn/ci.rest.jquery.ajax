<?php



/**
 * UserStatusCodes
 *
 * @Table(name="user_status_codes")
 * @Entity
 */
class UserStatusCodes
{
    /**
     * @var integer $idUserStatusCode
     *
     * @Column(name="id_user_status_code", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idUserStatusCode;

    /**
     * @var string $userStatusCode
     *
     * @Column(name="user_status_code", type="string", length=32, nullable=true)
     */
    private $userStatusCode;


}