<?php



/**
 * CustomerTypes
 *
 * @Table(name="customer_types")
 * @Entity
 */
class CustomerTypes
{
    /**
     * @var integer $idCustomerType
     *
     * @Column(name="id_customer_type", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idCustomerType;

    /**
     * @var string $customerTypeValue
     *
     * @Column(name="customer_type_value", type="string", length=32, nullable=false)
     */
    private $customerTypeValue;


}