<?php



/**
 * Invoices
 *
 * @Table(name="invoices")
 * @Entity
 */
class Invoices
{
    /**
     * @var integer $idInvoice
     *
     * @Column(name="id_invoice", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idInvoice;

    /**
     * @var string $invoiceDetail
     *
     * @Column(name="invoice_detail", type="string", length=128, nullable=false)
     */
    private $invoiceDetail;

    /**
     * @var datetime $issuedAt
     *
     * @Column(name="issued_at", type="datetime", nullable=true)
     */
    private $issuedAt;

    /**
     * @var InvoiceStatusCodes
     *
     * @ManyToOne(targetEntity="InvoiceStatusCodes")
     * @JoinColumns({
     *   @JoinColumn(name="fid_invoice_status_code", referencedColumnName="id_invoice_status_code")
     * })
     */
    private $fidInvoiceStatusCode;

    /**
     * @var Orders
     *
     * @ManyToOne(targetEntity="Orders")
     * @JoinColumns({
     *   @JoinColumn(name="fid_order", referencedColumnName="id_order")
     * })
     */
    private $fidOrder;


}