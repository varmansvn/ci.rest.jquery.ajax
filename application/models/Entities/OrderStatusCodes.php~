<?php



/**
 * OrderStatusCodes
 *
 * @Table(name="order_status_codes")
 * @Entity
 */
class OrderStatusCodes
{
    /**
     * @var integer $idOrderStatusCode
     *
     * @Column(name="id_order_status_code", type="integer", nullable=false)
     * @Id
     * @GeneratedValue(strategy="IDENTITY")
     */
    private $idOrderStatusCode;

    /**
     * @var string $orderStatusCodeValue
     *
     * @Column(name="order_status_code_value", type="string", length=32, nullable=false)
     */
    private $orderStatusCodeValue;


}